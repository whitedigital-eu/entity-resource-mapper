{
    "name": "whitedigital-eu/entity-resource-mapper-bundle",
    "description": "Bundle to map Doctrine entity object to and from API resource object.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Raitis Rasmanis",
            "email": "raitis.rasmanis@whitedigital.eu"
        },
        {
            "name": "Andis Cirulis",
            "email": "andis.cirulis@whitedigital.eu"
        },
        {
            "name": "Davis Zalitis",
            "email": "davis.zalitis@whitedigital.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "WhiteDigital\\EntityResourceMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WhiteDigital\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "api-platform/core": "^3.2",
        "doctrine/annotations": "*",
        "doctrine/collections": "*",
        "doctrine/common": "^3.4",
        "doctrine/dbal": "^3.7",
        "doctrine/orm": "^2.16",
        "doctrine/persistence": "*",
        "phpdocumentor/reflection-docblock": "*",
        "psr/log": "*",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/doctrine-bridge": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/maker-bundle": "*",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/security-core": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/service-contracts": "*",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^10.0",
        "whitedigital-eu/config-pack": "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    }
}
